                    div(class="col-span-2 row-span-3")
                    div(class="col-span-3 row-span-3")
                        img(class="shadow" loading="lazy" src="img/Ambassador_Toys/toys_works/ChinesePoster.jpg", alt="Chinese Poster Image" style="margin-top: -360px;" width="183" height="275")
                    div(class="col-span-7 row-span-3")
