    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" media="print"  onload="this.media='all'"/>
    style.
        .animatedLogo {
            animation: zoomInDown;
            animation-duration: 3.5s;
            animation-delay: 1.2s;
            animation-fill-mode: both;
        }
        .animate__heroBg {
            animation: fadeInDown;
            animation-duration: 4s;
            animation-iteration-count: 1;
            animation-fill-mode: both;
        }
        .stars_fadein {
            animation: fadeIn;
            animation-duration: 5s;
            animation-iteration-count: 1;
            animation-fill-mode: both;
        }


        .hero {
            min-height: 340px; }
            @media (min-width: 480px) {
                .hero {
                min-height: 400px; } }
            @media (min-width: 640px) {
                .hero {
                min-height: 450px; } }
            @media (min-width: 768px) {
                .hero {
                min-height: 500px; } }

        .stars {
        filter: drop-shadow(0 0 1px rgba(255, 255, 255, 0.507)) drop-shadow(0 0 5px #fff) drop-shadow(0 0 20px #fff); }

        .star {
            animation-name: starTwinkle;
            animation-iteration-count: infinite;
            animation-fill-mode: both;
            animation-delay: var(--delay);
            animation-duration: var(--duration);
            top: var(--top);
            left: var(--left);
            width: var(--width);
            height: var(--width);
            clip-path: polygon(57% 41%, 100% 50%, 57% 57%, 50% 100%, 43% 57%, 0 50%, 43% 42%, 50% 0); }

        @keyframes starTwinkle {
            0% {
                transform: scale(0.8);
                background: rgba(255, 255, 255, 0.7);
                animation-timing-function: ease-in; }
            50% {
                transform: scale(0.9);
                background: rgba(255, 255, 255, 0.7);
                animation-timing-function: ease-in; }
            60% {
                transform: scale(1.4);
                background: #fff;
                animation-timing-function: ease-out; }
            70% {
                transform: scale(0.9);
                background: rgba(255, 255, 255, 0.6); }
            100% {
                transform: scale(0.8);
            background: rgba(255, 255, 255, 0.7); }
        }

        .moon {
            width: 18vw;
            top: 10%;
            left: auto;
            right: 15%;
            animation: light 12s linear infinite both;
            filter: drop-shadow(0 0 60px #fff); }
        @media (min-width: 480px) {
            .moon {
            width: 16vw; } }
        @media (min-width: 640px) {
            .moon {
            width: 13vw; } }
        @media (min-width: 768px) {
            .moon {
            width: 123px;
            right: auto;
            left: 15%; } }

        @keyframes light {
            from {
                filter: brightness(100%); }
            50% {
                filter: brightness(200%) blur(0.5px) drop-shadow(0 0 30px #fff5bd); }
            to {
            filter: brightness(100%); } }

        .cloud {
            left: var(--left);
            top: var(--top);
            animation: animateCloud linear infinite both;
            animation-duration: calc(120s * var(--duration));
            animation-delay: calc(var(--delay) * 12);
            filter: blur(1px);
        }

        @keyframes animateCloud {
            0% {
                transform: translateX(-500px); }
            100% {
            transform: translateX(2000px); }
        }

        .toys-works-grid a {
            filter: drop-shadow(0 0 5px hsla(0, 0%, 0%, .3))
        }
        .toys-works-grid a img:hover {
            animation: swing;
            animation-iteration-count: 1;
            animation-fill-mode: both;
            animation-duration: 1s;
            }

        .butterfly:hover {
            animation: rubberBand;
            animation-iteration-count: 1;
            animation-fill-mode: both;
            animation-duration: 0.7s;
            animation-timing-function: ease-out;
        }
        .lightbulb:hover {
            animation: tada;
            animation-iteration-count: 1;
            animation-fill-mode: both;
            animation-duration: 1s;
            filter: drop-shadow(0 0 1px rgba(255, 255, 112, 0.509)) invert(1) drop-shadow(0 0 2px #ffff70) drop-shadow(0 0 5px #ffff70) drop-shadow(0 0 10px #ffff70);
        }
        .blue-bg {
            background-image: radial-gradient(circle at center, #1784c1 40%, #264783);
            box-shadow: 0 0 150px rgba(17, 41, 85, 0.5) inset;
        }
        .blue-bg::after {
            background-image: radial-gradient(circle at center, #1784c1 40%, #264783);
            box-shadow: 0 0 150px hsla(219, 67%, 20%, 0.5) inset;
            background: url(media/ambassador-toys/blueBGpattern.svg);
        }

