extends ./layout/base
block variables
    - let pageTitle = 'Tungsten Advertising Case Study | New Delhi Restaurant'
    - let metaDescription = 'Learn how Tungsten Advertising helped New Delhi Restaurant'
    - let mediaFolder = 'media/new-delhi/';
    - let currentUrl = 'new-delhi';
    - let ogImage = '../media/projects/new-delhi/new-delhi@936.jpg'

    - let headerShadow = false;

prepend preload
append preload
    link(rel="preload" as="image" href="../media/new-delhi/figure.svg" fetchpriority="high")
    link(rel="preload" as="image" href="../media/new-delhi/noise.svg" fetchpriority="high")
    link(rel="preload" as="image" href="../media/new-delhi/new-delhi-bg@1024.jpg" imagesrcset="media/new-delhi/new-delhi-bg@640.jpg 640w,media/new-delhi/new-delhi-bg@768.jpg 768w,media/new-delhi/new-delhi-bg@1024.jpg 1024w,media/new-delhi/new-delhi-bg@1366.jpg 1366w,media/new-delhi/new-delhi-bg@1600.jpg 1600w,media/new-delhi/new-delhi-bg@1920.jpg 1920w" fetchpriority="high")
    link(rel="preload" as="image" href="media/new-delhi/Rajan@1x.png" srcset="media/new-delhi/Rajan@1x.png 1x,media/new-delhi/Rajan@2x.png 2x" imagesizes="50vw, (min-width: 1024px) 771px" fetchpriority="high")

append head
    style.
        :root {
            --c-new-delhi: #DBB778;
            --c-title-red: #7C2525;
            --paper-texture: repeating-linear-gradient(90deg, #EFECE6 0px, #EDEAE5 min(10px, 1vw), #EFECE6 min(20px, 2vw)), url("../media/new-delhi/noise.svg");
        }
        body {

        }
        main {
            overflow-x: hidden;
        }
        #ndr {
            border: clamp(5px, 2vw, 8px) solid #F1EEE6;
            filter: drop-shadow(0 10px 40px hsl(0deg 0% 0% / 35%));
            --margin: 30px;
            margin-right: var(--margin);
            margin-top: var(--margin);
            margin-bottom: var(--margin);
            --shift: -34px;
            margin-left: var(--shift);
            scale: 1.1;
            .plaque {
                position: absolute;
                left: 42.6%;
                top: 62%;
                scale: 0;
                rotate: 360deg;
                translate: -50% -50%;
                transition: all .4s ease;
                filter: saturate(1.1) contrast(1.2) brightness(1.2);
            }
            .bg-img {
                transition: all 1s ease;
            }
            &:hover .bg-img {
                filter: blur(2px) sepia(.6);
            }
            &:hover .plaque {
                scale: 1;
                rotate: 0deg;
                top: 35%;
                left: 20%;
            }
            @media (width > 380px) {
                --shift: -50px;
                scale: 1;
            }
            @media (width >= 480px) {
                width: 70vw;
                --shift: -150px;
            }
            @media (width >= 768px) {
                --shift: -280px;
                width: auto;
            }
            @media (width > 1024px) {
                --shift: -200px;
            }
        }
        #star-anise {
            width: 90px;
            --shift-right: -35px;
            margin-right: var(--shift-right);
            @media (width >= 640px) {
                width: 120px;
                --shift-right: -30px;
                margin-top: 0;
                shape-margin: 50px;
            }
            @media (width > 1023px) {
                width: 218px;
                --shift-right: -30px;
                margin-top: -100px;
                shape-margin: 100px;
            }
        }
        #fancy-figure-top {
            --shift: -7vw;
            margin-top: var(--shift);
            width: 92%;
            @media (width >= 768px) {
                --shift: max(-9vw, -94px);
            }
        }
        #bg-top, #bg-bottom {
            background: var(--paper-texture);
            background-blend-mode: hue, darken;
            z-index: 1;
            -webkit-mask: url('../media/new-delhi/figure.svg') no-repeat bottom center / contain;
            height: clamp(50px, 17vw, 221px);
        }

        #bg-top {
            top: 1px;
        }
        #bg-bottom {
            width: 70%;
            top: -1px;
            scale: 1 -1;

        }
        .article-container {
            background: var(--paper-texture);
            background-blend-mode: hue, darken;
            --article-container-radius: min(3vw, 25px);
            &.top {
                border-top-left-radius: var(--article-container-radius);
                border-top-right-radius: var(--article-container-radius);
            }
            &.bottom {
                border-bottom-left-radius: var(--article-container-radius);
                border-bottom-right-radius: var(--article-container-radius);
            }
        }

        #rajan {
            margin-right: -80px;
            margin-top: 760px;
            width: 60vw;
            @media (width > 360px) {
                margin-top: 700px;
            }
            @media (width > 400px) {
                margin-top: 650px;
            }
            @media (width > 450px) {
                margin-top: 600px;
            }
            @media (width > 500px) {
                margin-top: 550px;
            }
            @media (width > 550px) {
                margin-top: 500px;
            }
            @media (width > 600px) {
                margin-top: 450px;
                width: 65vw;
            }
            @media (width > 768px) {
                margin-top: 400px;
            }
            @media (width > 1023px) {
                margin-right: -120px;
                margin-top: 260px;
                width: 771px;
                shape-margin: 10px;
            }
        }




